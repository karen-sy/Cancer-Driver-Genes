#!/usr/bin/env bash

go install
dos2unix LinuxRuns/workflow.sh
dos2unix LinuxRuns/vcf2maf.py
dos2unix LinuxRuns/createGenome.sh
env GOOS=linux GOARCH=amd64 go build -o LinuxRuns/createDNA main.go
env GOOS=linux GOARCH=amd64 go build -o LinuxRuns/genome/createGenome genome/createGenome.go
cd ./genome
go install
